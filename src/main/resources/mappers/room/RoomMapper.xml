<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.sw.dao.RoomDAO">
  	
	<!-- 방 생성(추가) -->
	 <insert id="roomInsert" useGeneratedKeys="true" keyProperty="ro_pid">
	 	 insert into productroom (pr_pid,ro_name,ro_count,ro_description,ro_min_people,ro_max_people,ro_select,ro_s_price,ro_s_in_time,ro_s_out_time,ro_t_price,ro_t_in_time,ro_t_out_time)
	 	 values (#{pr_pid},#{ro_name},#{ro_count},#{ro_description},#{ro_min_people},#{ro_max_people},#{ro_select},#{ro_s_price},#{ro_s_in_time},#{ro_s_out_time},#{ro_t_price},#{ro_t_in_time},#{ro_t_out_time})
	 </insert>
	 
	 <select id="roomList" resultType="com.kh.sw.vo.RoomVO" parameterType="string">
	 	 select ro_pid,pr_pid,ro_name,ro_count,ro_description,ro_min_people,ro_max_people,ro_select,ro_s_price,ro_s_in_time,ro_s_out_time,ro_t_price,ro_t_in_time,ro_t_out_time
	 	 from productroom
	 	 where pr_pid = #{pr_pid}
	 </select>
  	
</mapper>
